{
    "name": "elephantsgroup/eg-cms",
    "description": "Yii2 CMS Template, added modules ",
    "keywords": ["yii", "yii2", "framework", "cms", "elephantsgroup"],
    "type": "project",
    "homepage": "http://elephtansgroup.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jalal Jaberi",
            "email": "proodoo@gmail.com"
        },
        {
            "name": "Arezou Zahedi Majd",
            "email": "arezoumajd@gmail.com"
        }
     ],
    "support": {
        "issues": "https://github.com/ElephantsGroup/eg-cms/issues?state=open",
        "source": "https://github.com/ElephantsGroup/eg-cms/"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2",
        "yiisoft/yii2-swiftmailer": "*",
        "elephantsgroup/eg-base": "~1",
        "elephantsgroup/eg-jdf": "~1",
        "elephantsgroup/eg-jdate": "~1",
		"elephantsgroup/eg-user": "~1",
        "elephantsgroup/eg-admin": "~1",
		"elephantsgroup/eg-stat": "~1",
		"elephantsgroup/eg-service-relation": "~1",
		"elephantsgroup/eg-follow": "~1",
		"elephantsgroup/eg-like": "~1",
		"elephantsgroup/eg-star-rating": "~1",
		"elephantsgroup/eg-comment": "~1",
		"elephantsgroup/eg-contact": "~1",
		"elephantsgroup/eg-blog": "~1",
		"elephantsgroup/eg-event": "~1",
		"elephantsgroup/eg-news": "~1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*"
    },
    "config": {
        "process-timeout": 1800,
         "secure-http": false,
		 "fxp-asset": {
			"installer-paths": {
				"bower-asset-library": "vendor/bower"
			}
		}
    }
}
